		: KSentence .	
	mb [post-decrement]: rule
		< k > [Deref(Loc:Nat) -- => V:Int] ...</ k >
		< mem >... Loc:Nat |-> [V:Int => _-Int_(V:Int, 1)] ...</ mem >
		: KSentence .
		
	
