	op cleanMap : Map Nat Nat -> Map .
	eq cleanMap((M:Map (N:Nat |-> ?)), N:Nat, sNat(N':Nat)) = cleanMap(M:Map, sNat(N:Nat), N':Nat) .
	eq cleanMap(M:Map, N:Nat, 0) = M:Map .
		
