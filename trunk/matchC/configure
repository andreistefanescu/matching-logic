#!/bin/bash

echo "load ${K_BASE}/core/maude/lib/k-prelude.maude
load theories/core-theory.maude
load theories/integer-theory.maude
load theories/sequence-theory.maude
load theories/multiset-theory.maude
load theories/tree-theory.maude
load theories/domain.maude
load config.maude


mod ML-BUILTIN-MODULE is
  including MATHEMATICAL-INT-BUILTIN-MODULE .
  including MATHEMATICAL-SEQUENCE-BUILTIN-MODULE .
  including MATHEMATICAL-MULTISET-BUILTIN-MODULE .
  including MATHEMATICAL-TREE-BUILTIN-MODULE .
  including MATHEMATICAL-DOMAIN-BUILTIN-MODULE .
  including CONFIG-BUILTIN-MODULE .
endm
" >lib/ml-prelude.maude

